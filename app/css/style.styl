@import 'rupture'
@import "variable/_function.styl"
@import "variable/_fonts.styl"
@import "variable/_ie.styl"
@import "variable/_helper.styl"
@import "variable/_default.styl"
@import "variable/config/_color.styl"
@import "variable/config/_typography.styl"
@import "variable/config/_btn.styl"
@import "variable/config/_input.styl"
@import "../module/**/*"

body
	@extend .type--main
	font-family $v-font
	color $dark
	line-height 1.2
	letter-spacing 0.01em

.main-cont
	center(1200px,10px)

.page
	f()
	fl()
	min-height 100vh

.content
	+below(1280px)
		padding: 40px 0
	flex-grow 1
	padding: 100px 0

.content--clear
	padding-top: 0

.content--gray
	background: #f8f8f8

.content--mid
	flex(mid)

// animate top menu
@keyframes slideInDown
	from
		transform: translate3d(0, -100%, 0)
		visibility: visible;
	to
		transform: translate3d(0, 0, 0)

.shrink
	position fixed
	top 0
	left 0
	background white
	width 100%
	margin 0
	animation:  slideInDown  1s 1 ease-in-out;
	z-index 100
	.header-logo--md
		display none
	.header-logo--sm
		display block
	.header-logo
		height: auto
		border 0
		&:before
			content ''
			display none

// animate top menu === end


.header
	+below(1280px)
		height: 50px
	height: 90px
	border-bottom 1px solid $border-color
	f()
	align-items: stretch
	z-index 100
.header-menu
	+below(1440px)
		width: auto
	flex(mid)
	width: 200px
	border-right 1px solid $border-color
.header-menu-btn
	+below(1280px)
		width: 100%
		br(0)
		height: 100%
		flex(mid)
	+below(640px)
		padding: 0
		width: 50px
		span
			display none
	@extend .btn--red
	@extend .btn--md
	f()
	align-items center
	padding: 12px 20px
	line-height: 1
	.icon
		+below(640px)
			margin-right: 0
		fill white
		size 20px
		margin-right: 10px
.header-nav
	+below(1440px)
		padding 0 1vw
	+below(1280px)
		display none
	padding 0 2vw
	width: 65%
	flex(between)
.header-location
	f()
	cp()
	align-items center
	.icon
		size 20px
		fill $mark
		margin-right: 10px
.header-nav__el
	white-space nowrap
	margin: 0 5px
	&:hover
		color $mark
.header-logo
	+below(1280px)
		width: 170px
		height: 70px
	+below(768px)
		height: 50px
	+below(640px)
		border 0
	width: 14vw
	height: 10vw
	max-width 270px
	max-height 195px
	background: white
	margin: 0 auto
	flex(mid)
	position relative
	border 1px solid $border-color
	br(50%)
	&:before
		+below(1280px)
			height: 48px
		+below(640px)
			display none
		content ''
		top 0
		left -2px
		width: calc(100% + 4px)
		height: 89px
		position absolute
		background: white
	img
		z-index 5
		width: 55%
.header-logo--md
	+below(1280px)
		display none
.header-logo--sm
	+below(1280px)
		display block
	display none
.header-contact
	+below(1560px)
		padding 0 1vw
	+below(1280px)
		display none
	f()
	align-items center
	padding 0 2vw
	.icon
		fill $mark
		size 24px
		margin-right: 15px
.header-contact-mobile
	+above(1281px)
		display none
	flex(mid)
	size 50px
	border-right 1px solid $border-color
	.icon
		size 24px
		fill $mark
.header-nav-toggle
	+above(1281px)
		display none
	flex(mid)
	size 50px
	border-left 1px solid $border-color
	.icon
		size 24px

//.header-contact__info

.header-contact__phone
	+below(1560px)
		font-size: 18px
	@extend .type--lg
	color $dark
	white-space nowrap
	font-weight 700
	display inline-block
	margin-bottom: 5px
	&:hover
		color $mark
.header-contact__time
	+below(1560px)
		font-size: 12px
		white-space nowrap
	color $gray
	whtie-space nowrap
.header-auth
	+below(1560px)
		padding 0 1vw
	+below(768px)
		width: 50px
		padding: 0
	flex(mid)
	cp()
	border-left 1px solid $border-color
	padding 0 2vw

.header-auth-false
	+below(1280px)
		span
			display none
		.icon
			margin-right: 0!important
	f()
	align-items center
	.icon
		size 24px
		fill $gray
		margin-right: 15px
.header-auth-true
	f()
	align-items center
//.hidden
.user-img
	+below(1280px)
		margin-right: 0
		size 30px
	size 40px
	br(5px)
	bg-full()
	margin-right: 15px
.user-name
	+below(1280px)
		display none
.header-cart
	+below(1560px)
		padding 0 1vw
	+below(1280px)
		background: $red
	+below(768px)
		display none
	f()
	cp()
	padding 0 2vw
	border-left 1px solid $border-color
	align-items center
.header-cart__icon
	.icon
		+below(1280px)
			fill white
		size 30px
		fill $red
	.is-open
		size 20px
		margin-left: 10px
	margin-right: 15px
.header-cart__val
	+below(1280px)
		color white
	white-space nowrap
	color $dark
	@extend .type--md
.header-cart__items
	+below(1280px)
		background: $mark
		color black
		size 20px
		round()
		font-size: 12px
		margin-bottom: 15px
	flex(mid)
	background: $red
	size 40px
	color white
	@extend .type--md
	br(15px)
	margin-left: 10px
	.icon
		fill white

.header-part
	f()
	width calc(50% - 7vw)

.header-part--right
	justify-content: flex-end

// slide menu
.slide-block
	+below(1280px)
		top: 50px //Высота хедера
		height: calc(100% - 50px)
	+below(480px)
		width: 100%
	position: fixed
	top 90px
	width: 360px
	tr(all)
	border-right 1px solid $border-color
	height: calc(100% - 90px)
	background: white
.slide-block-wrap
	overflow-y auto
	height: 100%
.slide-menu__list
	padding: 10%
.slide-block--left
	left 0
	transform translateX(-100%)

.slide-block--right
	right 0
	transform translateX(100%)

.slide-block--open
	transform translateX(0%)

.slide-block--cart
	border-left 1px solid $border-color

.slide-menu__list-el
	display: block
	margin-bottom: 10px
	tr(all)
	color $dark
	@extend .type--md
	font-weight 700
	&:hover
		color red
.body-fix
	position: fixed!important
	overflow-Y: scroll!important
	width:100%!important
// slide menu === end


.mobile-cart
	+above(769px)
		display none
	position: fixed
	bottom 20px
	left calc(50% - 25px)
	z-index 10
	flex(mid)
	br(20px)
	background: $red
	transition all .8s
	.icon
		size 24px
		fill white

.mobile-cart--hidden
	transform translateY(200px)


.mobile-cart-wrap
	size 50px
	position relative
	flex(mid)
	.mobile-cart__val
		size 20px
		background: $mark
		flex(mid)
		round()
		position absolute
		left calc(50% - 10px)
		top -10px
		font-size: 12px
		font-weight: 700

.is-open
	display none

.slide-block-toggle--open
	.is-close
		display none!important
	.is-open
		display block!important
	.header-cart__icon
		.is-close
			display none

// header basket
.basket-container
	//max-height: calc(70vh - 200px)
	position relative
	overflow hidden
	flex-grow 1

.basket
	f()
	fl()
	height: 100%
.basket__el
	+below(768px)
		fw()
		flex(mid)
		fl()
		position: relative
	display flex!important
	flex(between)
	padding: 15px 20px
	outline none!important
	border-bottom 1px solid $border-color
.basket__el-info
	f()
	align-items: flex-start
	+below(768px)
		width: 100%
.basket__img
	+below(768px)
		margin-right: 0
		height: 100px
		width: 100px
	size 50px
	br(5px)
	bg-full()
	background-size: contain
	margin-right: 20px

.basket__cont
	width calc(100% - 70px)

.basket__name
	@extend .type--md
	font-weight: 700
	display block
	f()
	margin-bottom: 5px
	width: 100%


.basket__delete
	margin-left: auto
	.icon
		cp()
		tr(all)
		size 20px
		fill $gray
		margin-left: 5px
		margin-top: -2px
		&:hover
			fill $mark

.basket__descr
	font-size: 15px
	line-height: 1.4
	margin-bottom: 10px
	color $gray

.basket__el-change
	flex(between)
	+below(640px)
		width: 100%
	width: 45%
.basket__price
	+below(768px)
		margin-top: 20px
		font-size: 20px
	color $dark
	white-space nowrap
	@extend .type--lg
	font-weight 700

.basket-title
	@extend .type--lg
	font-weight 700
	padding: 20px 0 0 20px

.basket-empty
	flex-grow 1
	flex(mid)
	fl()
.basket-empty__img
	margin-bottom: 20px
.basket-empty__text
	text-align center
	width: 80%
	line-height: 1.6
	color $gray
// header basket === end

// slide menu nav
.slide-menu-nav
	padding: 30px
	.header-location
		margin-bottom: 20px
		font-size: 16px
		.icon
			size 24px
	.header-contact
		display: flex !important
.slide-menu-auth
	cp()
	margin-bottom: 20px

.slide-menu-auth-false
	+below(1280px)
		span
			display none
		.icon
			margin-right: 0!important
	f()
	align-items center
	.icon
		size 24px
		fill $gray
		margin-right: 15px
.slide-menu-auth-true
	f()
	align-items center
.slide-menu-user-img
	size 30px
	br(5px)
	bg-full()
	margin-right: 15px
.slide-menu-nav__el
	margin-bottom: 20px
	display block
	font-size: 16px
.slide-menu-user-name
	font-size: 16px
// slide menu nav === end


// incr
.incr
	f()
	//height 40px
	position relative
	width 160px

.incr--small
	width: 80px
	.incr__nav
		size 35px
		.icon
			size 10px
	.incr__val
		width: 25px
		font-size: 12px
		font-weight: 700

.incr__nav
	flex(mid)
	br(10px)
	tr()
	size 35px
	border 2px solid $mark
	background white
	.icon
		size 12px
		fill $mark
	&:hover
		cp()
		border-color $mark
		background: $mark
		.icon
			fill white
	&:active
		.icon
			fill $red
		background-color $mark-light
.incr__val
	flex(mid)
	background white
	size 40px 35px
	font-size 20px
	position relative
	tr(all)
	-webkit-touch-callout none
	-webkit-user-select none
	-khtml-user-select none
	-moz-user-select none
	-ms-user-select none
	user-select none
// incr === end

.basket-footer
	padding: 25px
	border-top 1px solid $border-color
	margin-top: auto
.basket-total
	flex(between)
	margin-bottom: 20px

.basket-total__text
	color $dark
	@extend .type--lg
	font-weight 700

.basket-total__val
	color $dark
	@extend .type--xl
	font-weight 700

.basket-total-get
	@extend .btn--md
	@extend .btn--mark
	width: 100%
	font-weight: 700

// slider

.slider
	margin-top: 20px
.slider__el
	padding: 0 1%
	-webkit-touch-callout none
	-webkit-user-select none
	-khtml-user-select none
	-moz-user-select none
	-ms-user-select none
	user-select none
	outline none
.slider__el-img
	+below(1280px)
		padding: 20% 10%
	+below(768px)
		padding: 30% 10%
	br(20px)
	overflow hidden
	bg-full()
	padding: 15.5% 10%
// slider === end

//=== Внешний вид пагинации ===
.slick-dots
	+below(768px)
		bottom 10px
	width: 100%
	display: flex !important
	align-items: center
	justify-content: center
	bottom 20px
	li
		+below(720px)
			size 10px
			flex(mid)
	.dot
		+below(768px)
			flex(mid)
			size 10px
		background: white
		size 20px
		display: block
		br(5px)
		opacity: .5
	.slick-active
		.dot
			opacity: 1
//=== Внешний вид пагинации end===

// slider control
//Навигация внутри
.slider-wrap
	position relative
.slider-control
	+below(768px)
		size 30px
		br(10px)
		margin-top: -15px
	flex(mid)
	tr(all)
	position: absolute
	top 50%
	margin-top: -20px
	z-index 21
	background: white
	size 50px
	br(20px)
	cp()
	&:hover
		opacity: 1
		.icon
			fill $mark
	.icon
		+below(768px)
			size 10px
		size 14px
.slider-control--left
	left 14%
.slider-control--right
	right 14%
// slider control === end


// kind
.kind
	+below(1024px)
		display none
	display: flex
	fw()
	margin-bottom: 50px

.kind__el
	font-size: 14px
	padding: 10px 25px
	br(20px)
	border 1px dashed $gray
	white-space nowrap
	text-align center
	margin-right: 10px
	margin-bottom: 10px
	cp()
	tr()
	display block
	color $brown
	&:hover
		background: $brown
		border 1px solid $brown
		color white

.kind__el--active
	background: $brown
	border 1px solid $brown
	color white
// kind === end

// PRODUCT
.product-el
	+below(640px)
		bsh()
		border-radius 20px
	background-color #ffffff
	tr(all)
	position: relative
	f()
	fl()

.product-el--sm
	.product-footer
		flex(mid)
		fl()
	.product-price
		width: 100%
		text-align: center
		margin-bottom: 20px
	.product-add
		width: initial
	.product__title
		text-align: center


.product__img
	+below(640px)
		margin-bottom: 0
	margin-bottom: 20px
	flex(mid)
	img
		margin: 0 auto
		tr(all)
		width: 100%

.product__cont
	+below(640px)
		padding: 15px
		border-radius 0 0 20px 20px
		border 1px solid $border-color
		border-top 0
	f()
	fl()
	flex-grow 1

.product__img-el
	+below(640px)
		border-radius 20px 20px 0 0
	tr(all)
	width: 100%
	height: 100%
	bg-full()
	background-size: cover
	padding: 33% 10%
	br(20px)
.product__title
	margin-bottom: 20px
	color $dark
	tr(all)
	@extend .type--lg
	font-weight 600
	display: block
	&:hover
		color #A57F45

.product-box
	margin-bottom:auto

.product__text
	opacity .8
	line-height: 1.6
	tr()
	margin-bottom: 20px

.product-cont
	+below(1024px)
		grid-template-columns: repeat(3, 1fr)
		margin: 30px 0
		grid-gap 20px
	+below(640px)
		grid-template-columns: repeat(2, 1fr)
	+below(480px)
		grid-template-columns: 1fr
		overflow initial
	//flex(between)
	margin-bottom: 100px
	display: grid
	grid-gap 40px
	grid-template-columns: repeat(4, 1fr)
	overflow: hidden
.product-cont--five
	+below(1024px)
		grid-template-columns: repeat(4, 1fr)
		margin: 30px 0
	+below(640px)
		grid-template-columns: repeat(3, 1fr)
	+below(480px)
		grid-template-columns: 1fr 1fr
	grid-template-columns: repeat(5, 1fr)

.product-back__title
	@extend .type--lg
	up()

	padding: 40px 0
	text-align: center
	position: relative

.product-back__arrow
	flex(mid)
	round()
	position: absolute
	size 44px
	background: white
	left: 50%
	top 100%
	margin-left: -22px
	margin-top: -22px
	.icon
		size: 22px
		margin-top: 9px

.product-info
	+below(768px)
		padding: 30px 10px
	background: red
	padding: 30px
	padding-top: 45px
	margin-bottom: 30px

.product-type
	flex(between)
	margin-bottom: 20px
//.product-thin

//.product-fat

.product-type__el
	flex(mid)
	width: 45%
	cp()
	&:hover
		.product-type__name
			opacity 1
		.product-type__icon
			.icon
				opacity 1

.product-type__icon
	margin-right: 10px
	.icon
		size: 36px
		fill white
		opacity .6


.product-type__name
	font-size: 16px
	opacity .6
	color white
	font-weight:500

.product-type--active
	.product-type__name
		opacity 1
	.product-type__icon
		.icon
			opacity 1

.product-type-condition
	margin-bottom: 20px

.product-type-condition-row
	height: 5px;
	border-radius: 2px;
	background-color: #a70a13;
	position: relative
.product-type-condition-el
	tr(all)
	position: absolute
	height: 100%
	width: 50%
	top 0
	left: 0
	background: $mark
	border-radius: 2px;

.product-size
	flex(between)
	margin: 30px 0

.product-size__el
	round()
	flex(mid)
	fl()
	fw()
	tr(all)
	cp()
	size 60px
	border: 2px solid #ffffff;
	background-color: rgba(252, 202, 3, 0);
	opacity: 0.6;
	&:hover
		opacity: 1
		background: $mark
		border 0
		.product-size__val
			color black
		.product-size__type
			color black

.product-size__el--active
	opacity: 1
	background: $mark
	border 0
	.product-size__val
		color black
	.product-size__type
		color black

.product-size__val
	@extend .type--md
	font-family: "PF Beau Sans Pro";
	color white

.product-size__type
	color white

.product-width
	text-align: center
	color white
	font-size: 18px
	opacity .6

.product-footer
	flex(between)
	margin-top: 20px
.product-price
	+below(1200px)
		font-size: 14px
	+below(1024px)
		font-size: 24px
	+below(640px)
		font-weight: 700
	@extend .type--md
	background-color #ffffff
	@extend .type--lg
	font-weight 600

.product-get
	+below(1200px)
		font-size: 14px
	+below(768px)
		font-size: 12px
	//display: none
	cp()
	@extend .btn--md
	@extend .btn--light
	padding: 10px 20px
	&:hover
		background: $mark-light

.product-add
	+below(768px)
		font-size: 12px
	cp()
	@extend .type--sm
	tr(all)
	width: 50%
	text-align: center
	@extend .btn--md
	@extend .btn--light
	padding: 5px 20px
	flex(mid)
	height: 40px
	white-space nowrap
	&:hover
		background: $mark

.product-incr
	+below(1200px)
		padding: 8px 5px
		.incr
			height: 30px
		.incr__nav
			size 30px
		.incr__val
			height: 30px
	flex(mid)
	width: 50%
	display: none
	height: 40px
	.incr
		width: auto

.product-footer-numb
	.product-price
		border-right: 0
	.product-incr
		display: flex
	.product-get
		display: none

.product-size-wrap--thin
	display: none

.size-disable
	opacity .2
	pointer-events none
	&:hover
		cursor none

.animate-basket
	animation: bounceIn 0.3s 2 ease-in-out

.size
	display: grid
	grid-gap 5px
	grid-template-columns: repeat(auto-fit, minmax(3rem,1fr))
	padding: 2px
	br(20px)
	border 1px dashed $mark

.size--lg
	br(30px)
	.size__el
		@extend .type--md
		padding: 20px 10px
		font-weight: 700
		&:first-child
			border-radius 30px 0 0 30px
		&:last-child
			border-radius 0 30px 30px 0

.size--column
	+below(640px)
		fw()
		.size__el
			width: 100%
			border-radius 30px!important

.size--sm
	display: inline-flex
	.size__el
		padding: 8px 20px


.size--fluid
	@extend .size--lg
	f()
	.size__el
		flex-grow 1
		font-weight: 700
		font-size: 14px

.size__el
	+below(480px)
		font-weight:700
	cp()
	tr()
	font-size: 14px
	padding: 8px 10px
	text-align: center
	line-height: 1
	&:first-child
		border-radius 20px 0 0 20px
	&:last-child
		border-radius 0 20px 20px 0
	&:hover
		border-color $mark
.size__el--active
	background: $mark

.product-el--add
	br(10px)
	padding: 5px 10px
	flex(mid)
	position absolute
	top: 10px
	right: 10px
	background: white
	z-index 10
	.icon
		margin-right: 3px
		size 16px

.product-el--add-val
	font-weight: 700

.product-weight
	margin-right auto
	margin-left: 10px
	color $gray

.item-add
	cp()
	flex(mid)
	tr()
	size 50px
	border-radius 20px
	border 2px solid #ffc400
	background-color #ffffff
	&:hover
		background: $mark
		.icon
			fill white
	&:active
		.icon
			fill $red
	.icon
		size 16px
		fill $mark

